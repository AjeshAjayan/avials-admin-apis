{
  "kind": "collectionType",
  "collectionName": "shop_users",
  "info": {
    "name": "Shop User",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "shop_name": {
      "type": "string"
    },
    "shop_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "shop_cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "about_shop": {
      "type": "richtext"
    },
    "date_of_join": {
      "type": "datetime"
    },
    "is_closed": {
      "type": "boolean"
    },
    "is_expired": {
      "type": "boolean"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "place": {
      "model": "place"
    },
    "latitude": {
      "type": "string"
    },
    "longitude": {
      "type": "string"
    },
    "is_verified": {
      "type": "boolean",
      "default": false
    },
    "shop_plan": {
      "model": "shop-plans"
    },
    "order_child": {
      "via": "shop_user",
      "model": "order-child"
    }
  }
}
