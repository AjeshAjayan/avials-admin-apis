{
  "kind": "collectionType",
  "collectionName": "managers",
  "info": {
    "name": "Managers",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "fullname": {
      "type": "string",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "default": false
    },
    "place": {
      "model": "place"
    },
    "is_terminated": {
      "type": "boolean",
      "default": false
    },
    "is_resigned": {
      "type": "boolean",
      "default": false
    },
    "date_of_birth": {
      "type": "date"
    },
    "date_of_joining": {
      "type": "datetime"
    },
    "is_verified": {
      "type": "boolean",
      "default": false
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "places": {
      "collection": "place",
      "via": "managers",
      "dominant": true
    },
    "profile_pic": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "users_permissions_role": {
      "plugin": "users-permissions",
      "model": "role"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    }
  }
}
