{
  "kind": "collectionType",
  "collectionName": "shop_products",
  "info": {
    "name": "Products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "product_sub_category": {
      "model": "product-sub-category"
    },
    "added_date": {
      "type": "datetime"
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "float"
    },
    "stock": {
      "type": "float"
    },
    "virtual_stock": {
      "type": "float"
    },
    "is_wholesale": {
      "type": "boolean",
      "default": false
    },
    "discount": {
      "type": "float",
      "max": 90,
      "min": 0,
      "default": 0
    },
    "product_images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "shop_user": {
      "model": "shop-user"
    },
    "public_user": {
      "model": "public-user"
    },
    "is_shop_product": {
      "type": "boolean"
    }
  }
}
